<div class="container-fluid">
  <div class="row">
    <div class="col-md-12 text-center">
<br><br>
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit 'Log In'%>
<% end %>
<br><br>
<%= link_to "New User? Sign Up Now!", new_user_path %>
    </div>
  </div>
</div>
