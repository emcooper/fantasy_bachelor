<h1>Sign Up!</h1>

<%= form_for @user do |f| %>
  <%= f.label :team_name %>
  <%= f.text_field :team_name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit "Sign Up!" %>
<% end %>
